{
    "name": "your-name-here/NagiosModule",
    "description": "NagiosModule plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NagiosModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NagiosModule\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
