{
    "name": "open-itcockpit/NewModule",
    "description": "NewModule plugin for openITCOCKPIT",
    "type": "cakephp-plugin",
    "license": "Dual licensed",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "NewModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NewModule\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
