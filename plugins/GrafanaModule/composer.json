{
    "name": "open-itcockpit/GrafanaModule",
    "description": "GrafanaModule plugin for openITCOCKPIT",
    "type": "cakephp-plugin",
    "license": "Dual licensed",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "GrafanaModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrafanaModule\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
